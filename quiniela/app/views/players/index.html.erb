<p style="color: green"><%= notice %></p>
<% store_location %>


<h1>Players</h1>

<div id="players">
  <% @players.each do |player| %>
    <%= "Player #" + player.id.to_s%> <br>
    <%= render player %>
    <p>
      <%= link_to "Show this player", player %>
    </p>
  <% end %>
</div>

<%= link_to "New player", new_player_path %>
